all: gateway door motion keychain securitysystem database

gateway: gateway.c
	gcc gateway.c -pthread -o gateway

door: /door.c
	gcc door.c -pthread -o door

motion: motion.c
	gcc motion.c -pthread -o motion

keychain: keychain.c
	gcc keychain.c -pthread -o keychain

securitysystem: securitysystem.c
	gcc securitysystem.c -o securitysystem

database: database.c
	gcc database.c -pthread -o database
